rget.files[0];\n      console.log(file.webkitRelativePath);\n      let name = file.webkitRelativePath;\n      axios.post('http://localhost:8077/sscm/saveresult', name).then(function (response) {\n        console.log(response.data);\n        if (response.data == 0) {\n          _this.$message({\n            message: '保存失败！',\n            type: 'error'\n          });\n        } else {\n          _this.$message({\n            message: '保存成功！',\n            type: 'success'\n          });\n        }\n      });\n      event.target.value = '';\n    },\n    save() {\n      this.$refs.saveres.click();\n    }\n  }\n};","map":{"version":3,"mappings":"AAeA,OAAOA,KAAI,MAAO,OAAO;AAEzB,eAAe;EACbC,IAAI,EAAE,KAAK;EACXC,UAAU,EAAE,CAEZ,CAAC;EAGDC,IAAI,GAAG;IACL,OAAO,CAEP;EACF,CAAC;EAEDC,OAAO,EAAE;IACPC,IAAI,CAACC,IAAI,EAAE;MACT,IAAI,CAACC,OAAO,CAACC,OAAO,CAACF,IAAI,CAAC;IAC5B,CAAC;IACDG,KAAK,GAAG;MACNC,MAAM,CAACC,QAAQ,CAACC,IAAG,GACf,kBAAiB;IACvB,CAAC;IAEDC,KAAK,GAAG;MACN,IAAIC,KAAI,GAAI,IAAI;MAChB,IAAI,CAACC,OAAO,CAAC,6CAA6C,EAAE,OAAO,EAAE;QACnEC,iBAAiB,EAAE,KAAK;QACxBC,gBAAgB,EAAE;MACpB,CAAC,CAAC,CAACC,IAAI,CAAC,CAAC;QAAEC;MAAM,CAAC,KAAK;QACrBnB,KAAK,CAACoB,IAAI,CAAC,mCAAmC,EAAED,KAAK,CAAC,CAACD,IAAI,CAAC,UAAUG,QAAQ,EAAC;UAC7EC,OAAO,CAACC,GAAG,CAACF,QAAQ,CAAClB,IAAI;UAEzB,IAAGkB,QAAQ,CAAClB,IAAI,IAAE,CAAC,EAAC;YAClBW,KAAK,CAACU,QAAQ,CAAC;cACbC,OAAO,EAAE,OAAO;cAChBC,IAAI,EAAE;YACR,CAAC,CAAC;UACJ,OACI;YACFZ,KAAK,CAACU,QAAQ,CAAC;cACbC,OAAO,EAAE,OAAO;cAChBC,IAAI,EAAE;YACR,CAAC,CAAC;UACJ;QACF,CAAC,CAAC;MAEJ,CAAC,CAAC,CAACC,KAAK,CAAC,MAAM;QACb,IAAI,CAACH,QAAQ,CAAC;UACZE,IAAI,EAAE,MAAM;UACZD,OAAO,EAAE;QACX,CAAC,CAAC;MACJ,CAAC,CAAC;IACJ,CAAC;IAEDG,SAAS,CAACC,KAAK,EAAE;MACf;MACA,IAAInB,MAAM,CAACoB,UAAU,EAAE;QACrB,IAAIC,IAAG,GAAIF,KAAK,CAACG,MAAM,CAACC,KAAK,CAAC,CAAC,CAAC;QAChC,IAAIC,MAAK,GAAI,IAAIJ,UAAU,EAAE;QAC7BI,MAAM,CAACC,MAAM,GAAGC,KAAK,IAAG;UACtB;UACA;UACAd,OAAO,CAACC,GAAG,CAACa,KAAK,CAACJ,MAAM,CAACK,MAAM,CAAC;UAChCrC,KAAK,CAACoB,IAAI,CAAC,sCAAsC,EAACgB,KAAK,CAACJ,MAAM,CAACK,MAAM,CAAC,